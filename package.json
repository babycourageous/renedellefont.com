{
  "name": "renedellefont.com",
  "description": "Personal portfolio and random musings about front end development.",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/babycourageous/11ty-netlify-starter.git",
  "author": "Rene Dellefont <renedellefont@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn run dev",
    "dev": "NODE_ENV=development npm-run-all --serial build:assets --parallel serve watch:*",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "chokidar 'src/_assets/css/**/*' 'tailwind.config.js' --command 'run-s build:assets:css'",
    "watch:js": "rollup -c -w",
    "watch:svg": "chokidar 'src/_assets/svg/**/*' --command 'run-s build:assets:svg'",
    "serve": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "build": "NODE_ENV=production npm-run-all --sequential build:assets build:11ty",
    "build:assets": "npm-run-all --parallel build:assets:*",
    "build:assets:css": "postcss src/_assets/css/index.css -o _site/css/styles.css",
    "build:assets:js": "rollup -c",
    "build:assets:svg": "svg-sprite --svg-xmldecl=false --svg-doctype=false --symbol=true --symbol-inline=true --symbol-dest=src/_includes src/_assets/svg/*.svg",
    "build:11ty": "ELEVENTY_ENV=production npx eleventy"
  },
  "dependencies": {
    "date-fns": "^2.6.0",
    "netlify-lambda": "^1.6.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.6.5",
    "babel-eslint": "^10.0.3",
    "browser-sync": "^2.26.7",
    "chokidar-cli": "^2.1.0",
    "cssnano": "^4.1.10",
    "eleventy-plugin-toc": "^1.0.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "prettier": "^1.18.2",
    "prism-svelte": "^0.4.6",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "svg-sprite": "^1.5.0",
    "tailwindcss": "^1.1.2"
  }
}
